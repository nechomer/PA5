class Fibonacci {
    static void main(string[] args) {
        string s = args[0];
        int n;
        int r;

        n = Library.stoi(s,-1);
        r = Fibonacci.fib(n);

        Library.printi(r);
        Library.println("");
    }

    static int fib(int n) {
        if (n < 2)
            return n;
        return Fibonacci.fib(n-1) + Fibonacci.fib(n-2);
    }
}
